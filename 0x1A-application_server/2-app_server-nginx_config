server {
        listen 80;
        listen [::]:80;

        server_name 100.25.198.215;

        add_header X-Served-By 625-web-01;

        location = /airbnb-onepage/ {
          proxy_pass http://127.0.0.1:5000/airbnb-onepage/;
        }

        error_page 404 /404.html;
        location /404 {
          root /var/www/html;
          internal;
        }
}
